setDatasetIfUndefined(snippetEl,"templateKey",defaultTemplate.key);this.updateTemplate(snippetEl,defaultTemplate);}else{let selectedFilterId=snippetEl.dataset["filterId"];if(Object.keys(dynamicFilters).length>0){if(!snippetEl.dataset.numberOfRecords){snippetEl.dataset["numberOfRecords"]=this.fetchedDynamicFilters[0].limit;}
