ibilityPlugin}=require("@website/builder/plugins/website_visibility_plugin");const{EditInteractionPlugin}=require("@website/builder/plugins/edit_interaction_plugin");const{AnimateOptionPlugin}=require("@website/builder/plugins/options/animate_option_plugin");const{BuilderComponentPlugin}=require("@html_builder/core/builder_component_plugin");const{BuilderActionsPlugin}=require("@html_builder/core/builder_actions_plugin");const{CoreBuilderActionPlugin}=require("@html_builder/core/core_builder_action_plugin");const{CarouselOptionTranslationPlugin}=require("@website/builder/plugins/carousel_option_translation_plugin");const{OverlayButtonsPlugin}=require("@html_builder/core/overlay_buttons/overlay_buttons_plugin");const{DropZonePlugin}=require("@html_builder/core/drop_zone_plugin");const{DropZoneSelectorPlugin}=require("@html_builder/core/dropzone_selector_plugin");const{CustomizeTabPlugin}=require("@html_builder/core/customize_tab_plugin");const{BuilderOverlayPlugin}=require("@html_builder/core/builder_overlay/builder_overlay_plugin");const{WebsiteSetupEditorPlugin}=require("@website/builder/plugins/setup_editor_plugin");const{ThemeTab}=require("@website/builder/plugins/theme/theme_tab");const{TranslateTableOfContentOptionPlugin}=require("@website/builder/plugins/options/table_of_content_option_plugin_translate");const{FieldChangeReplicationPlugin}=require("@html_builder/core/field_change_replication_plugin");const{BuilderContentEditablePlugin}=require("@html_builder/core/builder_content_editable_plugin");const{ImageFieldPlugin}=require("@html_builder/plugins/image_field_plugin");const{MonetaryFieldPlugin}=require("@html_builder/plugins/monetary_field_plugin");const{Many2OneOptionPlugin}=require("@html_builder/plugins/many2one_option_plugin");const{CustomizeTranslationTab}=require("@website/builder/plugins/translation_tab/customize_translation_tab");const{CustomizeTranslationTabPlugin}=require("@website/builder/plugins/translation_tab/customize_translation_tab_plugin")