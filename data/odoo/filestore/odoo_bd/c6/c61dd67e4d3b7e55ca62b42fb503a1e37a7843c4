odoo.define('@website/builder/plugins/font/add_font_dialog',['@web/core/network/rpc','@odoo/owl','@web/core/l10n/translation','@web/core/utils/hooks','@web/core/autocomplete/autocomplete','@web/core/dialog/dialog','@web/core/confirmation_dialog/confirmation_dialog'],function(require){'use strict';let __exports={};const{rpc}=require("@web/core/network/rpc");const{Component,useEffect,useRef,useState}=require("@odoo/owl");const{appTranslateFn}=require("@web/core/l10n/translation");const _t=(str,...args)=>appTranslateFn(str,"website",...args);const{useService}=require("@web/core/utils/hooks");const{AutoComplete}=require("@web/core/autocomplete/autocomplete");const{Dialog}=require("@web/core/dialog/dialog");const{ConfirmationDialog}=require("@web/core/confirmation_dialog/confirmation_dialog");class GoogleFontAutoComplete extends AutoComplete{setup(){super.setup();this.inputRef=useRef("input");this.sourcesListRef=useRef("sourcesList");useEffect((el)=>{el.setAttribute("id","google_font");},()=>[this.inputRef.el]);}
