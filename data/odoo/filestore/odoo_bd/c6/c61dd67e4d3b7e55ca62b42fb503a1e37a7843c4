/* /website/static/src/builder/plugins/options/ecomm_categories_showcase_option_plugin.js */
odoo.define('@website/builder/plugins/options/ecomm_categories_showcase_option_plugin',['@html_editor/plugin','@web/core/registry','@html_editor/utils/resource','@html_builder/utils/option_sequence','@html_builder/core/builder_action','@html_builder/core/utils'],function(require){'use strict';let __exports={};const{Plugin}=require("@html_editor/plugin");const{registry}=require("@web/core/registry");const{withSequence}=require("@html_editor/utils/resource");const{SNIPPET_SPECIFIC_BEFORE,END,VERTICAL_ALIGNMENT,}=require("@html_builder/utils/option_sequence");const{BuilderAction}=require("@html_builder/core/builder_action");const{BaseOptionComponent}=require("@html_builder/core/utils");const EcommCategoriesShowcaseOption=__exports.EcommCategoriesShowcaseOption=class EcommCategoriesShowcaseOption extends BaseOptionComponent{static template="website.EcommCategoriesShowcaseOption";static selector=".s_ecomm_categories_showcase";}
