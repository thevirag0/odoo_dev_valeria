odoo.define('@website_payment/website_builder/donation_option_plugin',['@html_builder/core/builder_action','@html_builder/core/utils','@html_builder/utils/option_sequence','@html_editor/plugin','@html_editor/utils/resource','@web/core/l10n/translation','@web/core/registry','@web/core/utils/render'],function(require){'use strict';let __exports={};const{BuilderAction}=require("@html_builder/core/builder_action");const{BaseOptionComponent}=require("@html_builder/core/utils");const{SNIPPET_SPECIFIC}=require("@html_builder/utils/option_sequence");const{Plugin}=require("@html_editor/plugin");const{withSequence}=require("@html_editor/utils/resource");const{appTranslateFn}=require("@web/core/l10n/translation");const _t=(str,...args)=>appTranslateFn(str,"website_payment",...args);const{registry}=require("@web/core/registry");const{renderToElement,renderToFragment}=require("@web/core/utils/render");const DonationOption=__exports.DonationOption=class DonationOption extends BaseOptionComponent{static template="website_payment.DonationOption";static selector=".s_donation";static cleanForSave(editingElement){delete editingElement.dataset.prefilledOptionsList;}}
