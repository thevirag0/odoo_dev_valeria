discard(){if(this.editor.shared.history.canUndo()){this.dialog.add(ConfirmationDialog,{title:_t("Discard all changes?"),body:_t("Are you sure you want to discard all your changes? Once you do, they're gone for good."),confirmLabel:_t("Discard changes"),cancelLabel:_t("Keep editing"),confirm:()=>this.props.builderProps.closeEditor(),cancel:()=>{},});}else{this.props.builderProps.closeEditor();}}
