async setOptionsDefaultValues(snippetEl,modelNameFilter,contextualFilterDomain=[]){await this.fetchDynamicFilters({model_name:modelNameFilter,search_domain:contextualFilterDomain,});await this.fetchDynamicSnippetTemplates(modelNameFilter);const dynamicFilters={};for(const dynamicFilter of this.fetchedDynamicFilters){dynamicFilters[dynamicFilter.id]=dynamicFilter;}
