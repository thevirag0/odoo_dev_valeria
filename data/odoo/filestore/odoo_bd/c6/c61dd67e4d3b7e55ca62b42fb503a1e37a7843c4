        <thead>
            <tr>
                <th/> 
                <t t-foreach="domState.data.datasets" t-as="dataset" t-key="dataset.key">
                    <th t-on-click.capture="onDatasetLabelClick">
                        <BuilderTextInput action="'updateDatasetLabel'" actionParam="dataset.key" style="domState.isPieChart ? '' : ('border: 2px solid ' + (getColor(dataset.backgroundColor) || getColor(dataset.borderColor)))"/>
                    </th>
                </t>

