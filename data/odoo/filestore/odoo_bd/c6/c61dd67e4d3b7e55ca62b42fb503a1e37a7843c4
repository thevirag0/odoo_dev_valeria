return true;}
async save(){this.editor.shared.operation.next(this._save.bind(this),{withLoadingEffect:false});}
async _save(){this.isSaving=true;await this.editor.shared.savePlugin.save({alwaysSkipAfterSaveHandlers:false});this.props.builderProps.closeEditor();this.isSaving=false;}
